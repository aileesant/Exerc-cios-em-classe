Algoritmo "NÚMEROS PARES -  FORMA 1"

// gerar e escrever os números pares entre 100 e 120

Var

   i : inteiro

Inicio
   para i de 100 ate 120 passo 1 faca
      escreval(i)
   fimpara

Fimalgoritmo

///////////////////////////

Algoritmo "NÚMEOS PARES - FORMA 2"

Var

   i : inteiro

Inicio

   para i de 100 ate 120 passo 2 faca
      escreval(i)
   fimpara

para i de 100 ate 120 passo 2 faca
se i % 2 = 0 entao
escreval(i)

fimse
fimpara

Fimalgoritmo
