pilha = [1,1,2,3,5]
print("Pilha: ", pilha)

pilha.append(8) # the 'append' is the same thing as 'push' that take the last-in out
print("Inserindo um elemento", pilha)

pilha.append(13)
print("Inserindo outro elemento", pilha)

pilha.pop() # the 'pop' take add something at the end/top
print("Removendo um elemento: ", pilha)

pilha.pop()
print("Removendo um elemento: ", pilha)

pilha.pop()
print("Removendo mais um elemento... ", pilha)
